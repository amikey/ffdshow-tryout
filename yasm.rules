<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Yasm"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="yasm"
			DisplayName="yasm"
			CommandLine="yasm -w -f win64 -m amd64 -DWIN64 -I$(InputDir) -o $(IntDir)\$(InputName).obj $(InputPath) "
			Outputs="$(IntDir)\$(InputName).obj"
			FileExtensions="*.yasm"
			ExecutionDescription="yasm assembling $(InputPath)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
