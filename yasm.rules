<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Yasm"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="yasm"
			DisplayName="yasm"
			CommandLine="yasm -f win32 -DPREFIX -DWIN32 -DHAVE_SSE3 -DUSE32 -Dalloc -Dexec -I$(InputDir) -o $(IntDir)\$(InputName).obj $(InputPath)"
			Outputs="$(IntDir)\$(InputName).obj"
			FileExtensions="*.asm"
			ExecutionDescription="yasm assembling $(InputPath)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
